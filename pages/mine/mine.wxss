.container_mine {
  display: flex;
  flex-direction: column;
  padding-top: 10px;
}

.line_all {
  background-color: #e6e6e6;
  width: 100%;
  height: 1rpx;
}

.container_order {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  height: 96rpx;
  background-color: #fff;
  padding-left: 10px;
  padding-right: 10px;
}

.text_order {
  font-size: 14px;
  color: #333;
}

.line_margin {
  background-color: #e6e6e6;
  width: 100%;
  height: 1rpx;
  margin-left: 10px;
}

.image_next {
  width: 30rpx;
  height: 30rpx;
  position: fixed;
  right: 10px;
}

.space{
  background-color: #f7f7f7;
  width: 100%;
  height: 10px;
}

.space_all{
   background-color: #f7f7f7;
  width: 100%;
  flex: 1;
}

